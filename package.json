{
  "name": "circle",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "true",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest || true",
    "selenium": "./node_modules/.bin/wdio wdio.conf.js",
    "android": "cordova run android",
    "flow": "flow"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./tools/setupJest.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/__tests__/*",
      "!**/__mocks__/*",
      "!src/store/*",
      "!src/index.js"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./node_modules/enzyme"
    ],
    "testPathDirs": [
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "MOBGEN",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "17.0.2",
    "ejs": "^2.5.6",
    "eslint-plugin-flowtype": "^2.34.1",
    "express": "^4.14.1",
    "flow-cli": "0.0.0-pre",
    "node-fetch": "^1.5.2",
    "npm-run-all": "^1.8.0",
    "object-assign": "4.1.0",
    "radium": "^0.19.1",
    "react": "^15.3.2",
    "react-bootstrap": "0.30.7",
    "react-bootstrap-carousel": "^1.2.0",
    "react-dom": "^15.3.2",
    "react-native": "0.44.2",
    "react-native-cli": "^2.0.1",
    "react-native-drawer": "^2.3.0",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-router-flux": "^3.39.1",
    "react-redux": "^4.4.5",
    "react-responsive-carousel": "^3.1.24",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.4",
    "react-slick": "^0.14.11",
    "redux": "^3.5.2",
    "redux-dialog": "0.0.9",
    "redux-saga": "^0.13.0",
    "redux-thunk": "^2.1.0",
    "slick-carousel": "^1.6.0",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.8.0",
    "browser-sync": "2.12.5",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-flowtype": "^2.31.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-meteor": "^3.5.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "flow-bin": "^0.44.2",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "jest-cli": "18.0.0",
    "jest-fetch-mock": "^1.0.6",
    "node-sass": "3.7.0",
    "npm-run-all": "^1.8.0",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "wdio-mocha-framework": "^0.4.0",
    "wdio-phantomjs-service": "^0.1.0",
    "webdriverio": "^4.2.8",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "engines": {
    "node": "7.x.x"
  }
}
